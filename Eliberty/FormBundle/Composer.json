{
    "name": "eliberty/form-bundle",
    "type": "symfony-bundle",
    "description": "Extra form types for your Symfony2 projects",
    "keywords": ["form", "extra form"],
    "license": "PROPRIETARY",
    "minimum-stability": "dev",
    "authors": [
        {
            "name": "philippe vesin",
            "email": "pvesin@eliberty.fr"
        }
    ],
    "require": {
        "php": ">=5.3.3",
        "genemu/form-bundle": "v2.2.2",
        "symfony/config": ">=2.1.0,<3.0",
        "symfony/dependency-injection": ">=2.1.0,<3.0",
        "symfony/http-kernel": ">=2.1.0,<3.0"
    },
    "extra": {
        "branch-alias":{
            "dev-master": "1.0.x-dev"
        }
    },
    "autoload": {
        "psr-4": { "Eliberty\\FormBundle\\": "" }
    },
    "target-dir": "Eliberty/FormBundle"
}